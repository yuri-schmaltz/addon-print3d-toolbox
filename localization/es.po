msgstr "Analizar"

msgid "Checks"
msgstr "Comprobaciones"

msgid "Scale To"
msgstr "Escalar a"

msgid "Scene Scale"
msgstr "Escala de Escena"

msgid "Copy Textures"
msgstr "Copiar Texturas"

msgid "Volume: {}³"
msgstr "Volumen: {}³"

msgid "Area: {}²"
msgstr "Área: {}²"

msgid "Non-manifold Edges: {}"
msgstr "Aristas No Desplegables: {}"

msgid "Bad Contiguous Edges: {}"
msgstr "Aristas Contiguas Incorrectas: {}"

